{
  "package": "com.application.demo",
  "name": "cnode-quickapp",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/common/img/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" },
    { "name": "system.fetch" },
    { "name": "system.file" },
    { "name": "system.webview" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "topic",
    "pages": {
      "index": {
        "component": "index",
        "path": "/"
      },
      "topic": {
        "component": "index",
        "path": "/topic"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "index": {
        "titleBarText": "cnode-quickapp",
        "menu": false,
        "titleBar": false
      }
    }
  }
}